{
    "name": "peachpayments/laravel-subscriptions",
    "description": "A Laravel package for handling Peach Payments subscriptions and recurring payments",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.5",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/notifications": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpstan/phpstan": "^1.10",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "PeachPayments\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PeachPayments\\Laravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PeachPayments\\Laravel\\PeachPaymentsServiceProvider"
            ],
            "aliases": {
                "PeachPayments": "PeachPayments\\Laravel\\Facades\\PeachPayments"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
